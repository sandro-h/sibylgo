{
  "name": "sibyl",
  "displayName": "Sibyl Todo",
  "description": "Sibyl vscode extension",
  "publisher": "mentalvary",
  "repository": "sibylgo",
  "version": "1.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sibylgo.clean",
        "title": "Clean done todos"
      },
      {
        "command": "sibylgo.trash",
        "title": "Trash done todos"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "sibylgo.clean",
          "when": "resourceFilename =~ /todo.txt$/",
          "group": "sibylgo@1"
        },
        {
          "command": "sibylgo.trash",
          "when": "resourceFilename =~ /todo.txt$/",
          "group": "sibylgo@1"
        }
      ]
    }
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package -o sibyl.vsix",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/node": "^10.14.17",
    "@types/request": "^2.48.5",
    "@types/vscode": "^1.34.0",
    "typescript": "^3.5.1",
    "vsce": "^1.71.0"
  }
}
